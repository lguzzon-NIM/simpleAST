# Set the default behavior, in case people don't have core.autocrlf set.
* text=auto

# All shell scripts must use LF
LICENSE text eol=lf
.editorconfig text eol=lf

.gitattributes text eol=lf
.gitignore text eol=lf


*.bash text eol=lf
*.csv text eol=lf
*.md text eol=lf
*.nim text eol=lf
*.nimble text eol=lf
*.nims text eol=lf
*.redis text eol=lf
*.sh text eol=lf
*.yaml text eol=lf
*.yml text eol=lf
